	Doi = {10.1101/460147},
	Elocation-Id = {460147},
	Eprint = {https://www.biorxiv.org/content/early/2018/11/02/460147.full.pdf},
	Journal = {bioRxiv},
	Publisher = {Cold Spring Harbor Laboratory},
	Title = {Comprehensive integration of single cell data},
	Url = {https://www.biorxiv.org/content/early/2018/11/02/460147},
	Year = {2018},
