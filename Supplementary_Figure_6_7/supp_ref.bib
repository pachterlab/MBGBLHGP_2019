	Journal = {Nucleic Acids Research},
	Journal1 = {Nucleic Acids Res},
	Month = {1/18/2020},
	Number = {D1},
	Pages = {D330--D338},
	Title = {The Gene Ontology Resource: 20 years and still GOing strong},
	Ty = {JOUR},
	Url = {https://doi.org/10.1093/nar/gky1055},
	Volume = {47},
	Year = {2018},
	Year1 = {2018/11/05/},
	Bdsk-Url-1 = {https://doi.org/10.1093/nar/gky1055}}

